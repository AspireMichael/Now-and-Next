{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Aspire Now and Next",
  "version": "1.0.0",
  "identifier": "uk.co.aspire.nowandnext",
  "build": {
    "frontendDist": "../src",
    "beforeBuildCommand": "",
    "beforeDevCommand": ""
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "Aspire Now and Next",
        "width": 1024,
        "height": 768,
        "minWidth": 400,
        "minHeight": 300,
        "resizable": true,
        "fullscreen": false,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}
